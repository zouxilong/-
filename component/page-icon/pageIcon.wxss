/* component/page-icon/pageIcon.wxss */

.pageIcon-wrapper {
  width: 600rpx;
  background: #f7f7f7;
  margin: 0 auto;
}

.pageIcon-content {
  width: 600rpx;
  display: flex;
  flex-flow: row wrap;
  justify-content: space-between;
}

.pageIcon-icon {
  width: 90rpx;
  height: 90rpx;
  background: #fff;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
}

.pageIcon-block {
  width: 150rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-bottom:60rpx;
}

.pageIcon-text {
  font-size: 20rpx;
  letter-spacing: 2rpx;
  width: 90rpx;
  height: 20rpx;
  margin-top: 14rpx;
}
